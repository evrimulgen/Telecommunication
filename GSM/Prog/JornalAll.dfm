  object qMain: TOraQuery
    SQLUpdate.Strings = (
      'UPDATE CONTRACTS'
      'SET'
      '  SEND_ACTIV = :SEND_ACTIV '
      'WHERE'
      '  CONTRACT_ID = :Old_CONTRACT_ID')
    SQL.Strings = (
      'SELECT DT.DOCUM_TYPE_NAME, T.* FROM'
      '('
      'SELECT AB.SURNAME, AB.NAME, AB.PATRONYMIC, AB.BDATE'
      '      ,F.FILIAL_NAME'
      '      ,O.OPERATOR_NAME'
      '      ,DECODE(AB.IS_VIP, 1, '#39#1044#1072#39') IS_VIP_NAME'
      '      ,T.TARIFF_NAME'
      '      ,T.CONNECT_PRICE'
      '      ,T.ADVANCE_PAYMENT'
      '      ,NVL(UN.USER_FIO, C.USER_LAST_UPDATED) USER_NAME'
      '      ,CNT_CITIZEN.COUNTRY_NAME COUNTRY_CITIZEN'
      '      ,CN.COUNTRY_NAME'
      '      ,RG.REGION_NAME'
      '      ,1 DOCUM_TYPE_ID'
      '      ,C.CONTRACT_ID DOCUM_ID'
      '      ,C.CONTRACT_DATE DOCUM_DATE'
      '     -- ,C.SEND_ACTIV'
      '      ,C.*'
      '  FROM CONTRACTS C'
      '      ,ABONENTS AB'
      '      ,FILIALS F'
      '      ,OPERATORS O'
      '      ,TARIFFS T'
      '      ,USER_NAMES UN'
      '      ,COUNTRIES CNT_CITIZEN'
      '      ,COUNTRIES CN'
      '      ,REGIONS RG'
      'WHERE C.ABONENT_ID = AB.ABONENT_ID (+)'
      '  AND C.FILIAL_ID = F.FILIAL_ID (+)'
      '  AND C.OPERATOR_ID = O.OPERATOR_ID (+)'
      '  AND C.TARIFF_ID = T.TARIFF_ID (+)'
      '  AND C.USER_LAST_UPDATED = UN.USER_NAME (+)'
      '  AND AB.CITIZENSHIP = CNT_CITIZEN.COUNTRY_ID (+)'
      '  AND AB.COUNTRY_ID = CN.COUNTRY_ID (+) '
      '  AND AB.REGION_ID = RG.REGION_ID (+)'
      ') T,'
      'DOCUM_TYPES DT'
      'WHERE T.DOCUM_TYPE_ID = DT.DOCUM_TYPE_ID (+)')
    ReadOnly = True
    RefreshOptions = [roAfterInsert, roAfterUpdate]
    IndexFieldNames = 'DOCUM_DATE;DOCUM_TYPE_ID;CONTRACT_NUM'
