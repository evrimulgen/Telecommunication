  object qMainSQL: TOraQuery
    SQL.Strings = (
      'SELECT DC.DOCUM_TYPE_ID'
      '      ,DC.DOCUM_ID'
      '      ,DC.DOCUM_DATE'
      '      ,DC.FILIAL_ID DOCUM_FILIAL_ID'
      '      ,DT.DOCUM_TYPE_NAME'
      '      ,AB.SURNAME, AB.NAME, AB.PATRONYMIC, AB.BDATE'
      '      ,F.FILIAL_NAME'
      '      ,O.OPERATOR_NAME'
      '      ,DECODE(AB.IS_VIP, 1, '#39#1044#1072#39') IS_VIP_NAME'
      '      ,C.ABONENT_TARIFF_OPTION'
      '      ,T.TARIFF_NAME'
      '      ,NVL(UN.USER_FIO, DC.USER_LAST_UPDATED) USER_NAME'
      '      ,NVL(UC.USER_FIO, DC.USER_CREATED) USER_CREATED'
      '      ,CNT_CITIZEN.COUNTRY_NAME COUNTRY_CITIZEN'
      '      ,CN.COUNTRY_NAME'
      '      ,RG.REGION_NAME'
      '      ,(SELECT CC.CONTRACT_CANCEL_ID'
      '          FROM CONTRACT_CANCELS CC'
      
        '         WHERE CC.CONTRACT_ID = C.CONTRACT_ID AND ROWNUM < 2) CO' +
        'NTRACT_CANCEL_ID'
      '      ,(SELECT CC.CONTRACT_CANCEL_DATE'
      '          FROM CONTRACT_CANCELS CC'
      
        '         WHERE CC.CONTRACT_ID = C.CONTRACT_ID AND ROWNUM < 2) CO' +
        'NTRACT_CANCEL_DATE'
      '      ,C.CONTRACT_NUM'
      '      ,C.START_BALANCE'
      '      ,C.SEND_ACTIV'
      '      ,C.DISCONNECT_LIMIT'
      '      ,DC.DOCUM_TYPE_ID '
      '      ,DC.DOCUM_ID '
      '      ,DC.CONFIRMED '
      '      ,DC.DOCUM_DATE '
      '      ,DC.CONTRACT_ID '
      '      ,DC.FILIAL_ID'
      '      ,DC.TARIFF_ID'
      
        '      ,NVL(DC.PHONE_NUMBER_FEDERAL, C.PHONE_NUMBER_FEDERAL) PHON' +
        'E_NUMBER_FEDERAL '
      
        '      ,NVL(DC.PHONE_NUMBER_CITY, C.PHONE_NUMBER_CITY) PHONE_NUMB' +
        'ER_CITY'
      '      ,DC.PHONE_NUMBER_TYPE'
      '      ,DC.SIM_NUMBER'
      '      ,DC.SUM_GET '
      '      ,DC.TYPE_NAME '
      '      ,DC.PARENT_PAYMENT_ID'
      '      ,DC.IS_DISTRIBUTED'
      '      ,DECODE(DC.REVERSESCHET,1,'#39#1044#1072#39','#39#1053#1077#1090#39') REVERSESCHET'
      '      ,T.TARIFF_CODE'
      '      ,T.IS_ACTIVE '
      '      ,T.CONNECT_PRICE '
      '      ,T.ADVANCE_PAYMENT '
      '      ,T.DAYLY_PAYMENT'
      '      ,T.DAYLY_PAYMENT_LOCKED'
      '      ,C.GOLD_NUMBER_SUM '
      '      ,(SELECT CCT.CONTRACT_CANCEL_TYPE_NAME'
      '          FROM CONTRACT_CANCELS CC, CONTRACT_CANCEL_TYPES CCT'
      '         WHERE CC.CONTRACT_ID = C.CONTRACT_ID'
      
        '           AND CC.CONTRACT_CANCEL_TYPE_ID = CCT.CONTRACT_CANCEL_' +
        'TYPE_ID AND ROWNUM < 2) CONTRACT_CANCEL_TYPE_NAME'
      '       ,AB.PASSPORT_SER '
      '       ,AB.PASSPORT_NUM '
      '       ,AB.PASSPORT_DATE '
      '       ,AB.PASSPORT_GET'
      '       ,AB.CITY_NAME '
      '       ,AB.STREET_NAME '
      '       ,AB.HOUSE'
      '       ,AB.KORPUS '
      '       ,AB.APARTMENT '
      '       ,AB.CONTACT_INFO '
      '       ,AB.CODE_WORD'
      '       ,AB.EMAIL '
      
        '  FROM CONTRACTS C, ABONENTS AB, FILIALS F, OPERATORS O, TARIFFS' +
        ' T'
      
        '      ,USER_NAMES UN, COUNTRIES CNT_CITIZEN, COUNTRIES CN, REGIO' +
        'NS RG, USER_NAMES UC'
      '      ,('
      
        '           SELECT 1 DOCUM_TYPE_ID, C.CONTRACT_ID DOCUM_ID, C.CON' +
        'FIRMED, C.CONTRACT_DATE DOCUM_DATE, C.CONTRACT_ID, C.FILIAL_ID, ' +
        'C.TARIFF_ID, C.PHONE_NUMBER_FEDERAL, '
      
        '           C.PHONE_NUMBER_CITY, C.PHONE_NUMBER_TYPE, C.SIM_NUMBE' +
        'R, TO_NUMBER(C.RECEIVED_SUM) SUM_GET, null TYPE_NAME, null REVER' +
        'SESCHET, C.USER_LAST_UPDATED, C.USER_CREATED, null PARENT_PAYMEN' +
        'T_ID, NULL IS_DISTRIBUTED'
      '           FROM CONTRACTS C'
      '         UNION ALL'
      
        '           SELECT 5 DOCUM_TYPE_ID, CC.CONTRACT_CANCEL_ID DOCUM_I' +
        'D, CC.CONFIRMED, CC.CONTRACT_CANCEL_DATE DOCUM_DATE, CC.CONTRACT' +
        '_ID, '
      
        '           CC.FILIAL_ID, TO_NUMBER(NULL) TARIFF_ID, TO_CHAR(NULL' +
        ') PHONE_NUMBER_FEDERAL, TO_CHAR(NULL) PHONE_NUMBER_CITY, TO_NUMB' +
        'ER(NULL) PHONE_NUMBER_TYPE, '
      
        '           TO_CHAR(NULL) SIM_NUMBER, (NVL(CC.SUM_GET, 0) - NVL(C' +
        'C.SUM_PUT, 0)) SUM_GET, null TYPE_NAME,null REVERSESCHET, CC.USE' +
        'R_LAST_UPDATED, CC.USER_CREATED, null PARENT_PAYMENT_ID, NULL IS' +
        '_DISTRIBUTED'
      '           FROM CONTRACT_CANCELS CC'
      '         UNION ALL'
      
        '           SELECT CCH.DOCUM_TYPE_ID, CCH.CONTRACT_CHANGE_ID DOCU' +
        'M_ID, CCH.CONFIRMED, CCH.CONTRACT_CHANGE_DATE DOCUM_DATE, CCH.CO' +
        'NTRACT_ID, '
      
        '           CCH.FILIAL_ID, CCH.TARIFF_ID, CCH.PHONE_NUMBER_FEDERA' +
        'L, CCH.PHONE_NUMBER_CITY, CCH.PHONE_NUMBER_TYPE, CCH.SIM_NUMBER,' +
        ' CCH.SUM_GET, null TYPE_NAME,null REVERSESCHET, '
      
        '           CCH.USER_LAST_UPDATED, CCH.USER_CREATED, null PARENT_' +
        'PAYMENT_ID, NULL IS_DISTRIBUTED'
      '           FROM CONTRACT_CHANGES CCH'
      '         UNION ALL'
      
        '           SELECT 0 DOCUM_TYPE_ID, RECEIVED_PAYMENTS.RECEIVED_PA' +
        'YMENT_ID DOCUM_ID, '
      
        '             DECODE(RECEIVED_PAYMENTS.PAYMENT_ANNUL_FLAG, 0, 1, ' +
        '0) CONFIRMED, '
      '             RECEIVED_PAYMENTS.PAYMENT_DATE_TIME DOCUM_DATE, '
      
        '             RECEIVED_PAYMENTS.CONTRACT_ID, RECEIVED_PAYMENTS.FI' +
        'LIAL_ID, '
      
        '             CONTRACTS.TARIFF_ID, RECEIVED_PAYMENTS.PHONE_NUMBER' +
        ' PHONE_NUMBER_FEDERAL, '
      
        '             CONTRACTS.PHONE_NUMBER_CITY, CONTRACTS.PHONE_NUMBER' +
        '_TYPE, CONTRACTS.SIM_NUMBER, '
      '             RECEIVED_PAYMENTS.PAYMENT_SUM SUM_GET,'
      
        '             RECEIVED_PAYMENT_TYPES.RECEIVED_PAYMENT_TYPE_NAME T' +
        'YPE_NAME,'
      '             RECEIVED_PAYMENTS.REVERSESCHET REVERSESCHET,'
      '             RECEIVED_PAYMENTS.USER_LAST_UPDATED,'
      '             RECEIVED_PAYMENTS.USER_CREATED,'
      '             RECEIVED_PAYMENTS.PARENT_PAYMENT_ID,'
      '             RECEIVED_PAYMENTS.IS_DISTRIBUTED'
      
        '           FROM RECEIVED_PAYMENTS, CONTRACTS, RECEIVED_PAYMENT_T' +
        'YPES '
      
        '             WHERE RECEIVED_PAYMENTS.IS_CONTRACT_PAYMENT = 0 /* ' +
        #1054#1087#1083#1072#1090#1099' '#1087#1088#1080' '#1089#1086#1089#1090#1072#1074#1083#1077#1085#1080#1080' '#1076#1086#1075#1086#1074#1086#1088#1072' '#1085#1077' '#1087#1086#1082#1072#1079#1099#1074#1072#1077#1084' */ AND '
      
        '               RECEIVED_PAYMENTS.CONTRACT_ID=CONTRACTS.CONTRACT_' +
        'ID '
      
        '               AND RECEIVED_PAYMENT_TYPES.RECEIVED_PAYMENT_TYPE_' +
        'ID(+)=RECEIVED_PAYMENTS.RECEIVED_PAYMENT_TYPE_ID'
      '       ) DC'
      '      ,DOCUM_TYPES DT'
      'WHERE DC.CONTRACT_ID = C.CONTRACT_ID '
      '  AND C.ABONENT_ID = AB.ABONENT_ID (+)'
      '  AND DC.FILIAL_ID = F.FILIAL_ID (+)'
      '  AND C.OPERATOR_ID = O.OPERATOR_ID (+)'
      '  AND DC.TARIFF_ID = T.TARIFF_ID (+)'
      '  AND DC.USER_LAST_UPDATED = UN.USER_NAME (+)'
      '  AND DC.USER_CREATED = UC.USER_NAME (+)'
      '  AND AB.CITIZENSHIP = CNT_CITIZEN.COUNTRY_ID (+)'
      '  AND AB.COUNTRY_ID = CN.COUNTRY_ID (+) '
      '  AND AB.REGION_ID = RG.REGION_ID (+)'
      '  AND DC.DOCUM_TYPE_ID = DT.DOCUM_TYPE_ID (+)')
    Left = 104
    Top = 86
  end
