  object qReceivedPayments: TOraQuery
    UpdatingTable = 'RECEIVED_PAYMENTS'
    SQLInsert.Strings = (
      'INSERT INTO RECEIVED_PAYMENTS'
      
        '  (PHONE_NUMBER, PAYMENT_SUM, PAYMENT_DATE_TIME, CONTRACT_ID, IS' +
        '_CONTRACT_PAYMENT, FILIAL_ID, REMARK, RECEIVED_PAYMENT_TYPE_ID, ' +
        'REVERSESCHET )'
      'VALUES'
      
        '  (:PHONE_NUMBER, :PAYMENT_SUM, :PAYMENT_DATE_TIME, :CONTRACT_ID' +
        ', :IS_CONTRACT_PAYMENT, :FILIAL_ID, :REMARK, :RECEIVED_PAYMENT_T' +
        'YPE_ID, :REVERSESCHET)')
    SQLDelete.Strings = (
      'DELETE FROM RECEIVED_PAYMENTS'
      'WHERE'
      '  RECEIVED_PAYMENT_ID = :Old_RECEIVED_PAYMENT_ID')
    SQLUpdate.Strings = (
      'UPDATE RECEIVED_PAYMENTS'
      'SET'
      
        '  PHONE_NUMBER = :PHONE_NUMBER, PAYMENT_SUM = :PAYMENT_SUM, PAYM' +
        'ENT_DATE_TIME = :PAYMENT_DATE_TIME, CONTRACT_ID = :CONTRACT_ID, ' +
        'IS_CONTRACT_PAYMENT = :IS_CONTRACT_PAYMENT, FILIAL_ID = :FILIAL_' +
        'ID, REMARK = :REMARK, RECEIVED_PAYMENT_TYPE_ID = :RECEIVED_PAYME' +
        'NT_TYPE_ID, REVERSESCHET = :REVERSESCHET, IS_DISTRIBUTED = :IS_D' +
        'ISTRIBUTED'
      'WHERE'
      '  RECEIVED_PAYMENT_ID = :Old_RECEIVED_PAYMENT_ID')
    SQLLock.Strings = (
      'SELECT * FROM RECEIVED_PAYMENTS'
      'WHERE'
      '  RECEIVED_PAYMENT_ID = :Old_RECEIVED_PAYMENT_ID'
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      
        'SELECT PHONE_NUMBER, PAYMENT_SUM, PAYMENT_DATE_TIME, CONTRACT_ID' +
        ', IS_CONTRACT_PAYMENT, FILIAL_ID, REMARK FROM, RECEIVED_PAYMENT_' +
        'TYPE_ID RECEIVED_PAYMENTS, REVERSESCHET, IS_DISTRIBUTED'
      'WHERE'
      '  RECEIVED_PAYMENT_ID = :RECEIVED_PAYMENT_ID')
    SQL.Strings = (
      'SELECT RECEIVED_PAYMENT_ID, '
      '       PHONE_NUMBER, '
      '       PAYMENT_SUM, '
      '       PAYMENT_DATE_TIME, '
      '       CONTRACT_ID, '
      '       IS_CONTRACT_PAYMENT, '
      '       FILIAL_ID, '
      '       PAYMENT_ANNUL_FLAG, '
      '       PAYMENT_ANNUL_DATE_TIME, '
      '       PAYMENT_ANNUL_REASON, '
      '       USER_WHO_ANNULATE, '
      '       USER_CREATED, '
      '       DATE_CREATED, '
      '       USER_LAST_UPDATED, '
      '       DATE_LAST_UPDATED, '
      '       REMARK, '
      '       RECEIVED_PAYMENT_TYPE_ID, '
      '       REVERSESCHET, '
      '       PARENT_PAYMENT_ID, '
      '       NVL(IS_DISTRIBUTED, 0) IS_DISTRIBUTED,'
      '       PAYMENT_PERIOD '
      ' FROM RECEIVED_PAYMENTS'
      'WHERE RECEIVED_PAYMENT_ID = :RECEIVED_PAYMENT_ID')
    Left = 168
    Top = 136
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'RECEIVED_PAYMENT_ID'
