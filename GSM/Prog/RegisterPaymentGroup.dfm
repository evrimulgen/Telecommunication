  object qInsReceivedPayments: TOraQuery
    SQL.Strings = (
      'INSERT INTO RECEIVED_PAYMENTS'
      
        '  (PHONE_NUMBER, PAYMENT_SUM, PAYMENT_DATE_TIME, CONTRACT_ID, IS' +
        '_CONTRACT_PAYMENT, FILIAL_ID, REMARK)'
      'SELECT c.PHONE_NUMBER_FEDERAL, '
      '       :PAYMENT_SUM*r.ratio,'
      '       :PAYMENT_DATE_TIME,'
      '       c.CONTRACT_ID,'
      '       :IS_CONTRACT_PAYMENT,'
      '       :FILIAL_ID,'
      '       :REMARK'
      'FROM v_contracts c,'
      '     (SELECT a.phone_number, a.bill_sum_new/b.bill_sum ratio'
      '      FROM'
      '        (SELECT v.PHONE_NUMBER, V.bill_sum_new'
      '         FROM V_BILL_FINANCE_FOR_GROUP_DET v'
      '         WHERE v.GROUP_ID = :GROUP_ID'
      '               AND v.year_month = :YEAR_MONTH_PAST) a,'
      '        (SELECT  g.BILL_SUM_NEW bill_sum'
      '         FROM V_BILL_FINANCE_FOR_GROUPS g'
      '         WHERE g.GROUP_ID = :GROUP_ID'
      '               AND g.year_month = :YEAR_MONTH_PAST) b'
      '          ) r  '
      'WHERE c.group_id = :GROUP_ID'
      '  AND c.PHONE_NUMBER_FEDERAL = r.phone_number')
    Left = 240
    Top = 40
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'PAYMENT_SUM'
      end
      item
        DataType = ftUnknown
        Name = 'PAYMENT_DATE_TIME'
      end
      item
        DataType = ftUnknown
        Name = 'IS_CONTRACT_PAYMENT'
      end
      item
        DataType = ftUnknown
        Name = 'FILIAL_ID'
      end
      item
        DataType = ftUnknown
        Name = 'REMARK'
      end
      item
        DataType = ftUnknown
        Name = 'GROUP_ID'
      end
      item
        DataType = ftUnknown
        Name = 'YEAR_MONTH_PAST'
      end>
  end
  object qUpdReceivedPayments: TOraQuery
    SQL.Strings = (
      'UPDATE RECEIVED_PAYMENTS'
      'SET'
      
        '  PAYMENT_SUM = :PAYMENT_SUM, PAYMENT_DATE_TIME = :PAYMENT_DATE_' +
        'TIME, FILIAL_ID = :FILIAL_ID, REMARK = :REMARK'
      'WHERE'
      '  RECEIVED_PAYMENT_ID = :RECEIVED_PAYMENT_ID')
    Left = 368
    Top = 40
    ParamData = <
