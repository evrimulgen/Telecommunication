  object qReport: TOraQuery
    SQL.Strings = (
      'SELECT V_ABONENT_BALANCES_WITHOUTSORT.*, t.tariff_code FROM'
      ' V_ABONENT_BALANCES_WITHOUTSORT,'
      '  tariffs t'
      'WHERE '
      '  V_ABONENT_BALANCES_WITHOUTSORT.tariff_id = t.tariff_Id(+) '
      '  and'
      '  (:OPERATOR_NAME IS NULL'
      
        '     OR V_ABONENT_BALANCES_WITHOUTSORT.OPERATOR_NAME = :OPERATOR' +
        '_NAME or (:OPERATOR_NAME ='#39#1042#1089#1077#39'))'
      
        '  AND ((:ACCOUNT_ID IS NULL)OR(V_ABONENT_BALANCES_WITHOUTSORT.AC' +
        'COUNT_ID=:ACCOUNT_ID) or (:ACCOUNT_ID =-1))'
      
        '  AND ((:IS_CREDIT IS NULL)OR(NVL(V_ABONENT_BALANCES_WITHOUTSORT' +
        '.IS_CREDIT_CONTRACT, 0) = :IS_CREDIT))')
    FetchRows = 250
