  object qOTTOK: TOraQuery
    Session = MainForm.OraSession
    SQL.Strings = (
      'select q.YEAR_MONTH'
      '      ,Q.PHONE_NUMBER_FEDERAL  -- '#1053#1054#1052#1045#1056
      '      ,q.TARIFF_NAME '
      '      ,q.ABON_TP_NEW'
      '      ,q.CONTRACT_DATE'
      '      ,q.CONTRACT_CANCEL_DATE'
      '      ,q.PERIOD      '
      
        '      ,(q.BILL_SUM_NEW - q.ROUMING_INTERNATIONAL - q.ROUMING_NAT' +
        'IONAL) LTV'
      '      ,(q.BILL_SUM_NEW - q.BILL_SUM_OLD) TARIFER_PEREPLATA'
      
        '      ,((q.BILL_SUM_NEW - q.ROUMING_INTERNATIONAL - q.ROUMING_NA' +
        'TIONAL)*0.2 + (q.BILL_SUM_NEW - q.BILL_SUM_OLD)) VYRUCHKA_AS'
      '      ,q.BILL_SUM_NEW'
      '      ,q.ROUMING_INTERNATIONAL'
      '      ,q.ROUMING_NATIONAL '
      '      ,q.BILL_SUM_OLD'
      'from '
      '(       '
      ' select        VOP.YEAR_MONTH'
      '              ,VOP.PHONE_NUMBER_FEDERAL'
      '              ,VOP.CONTRACT_DATE'
      '              ,VOP.CONTRACT_CANCEL_DATE'
      
        '              ,TRUNC ( MONTHS_BETWEEN ( nvl(VOP.CONTRACT_CANCEL_' +
        'DATE,sysdate) + 1, VOP.CONTRACT_DATE) ) PERIOD'
      '              ,(select TR.TARIFF_NAME'
      '                  from TARIFFS tr'
      '                 where TR.TARIFF_ID='
      '                 GET_PHONE_TARIFF_ID (VOP.PHONE_NUMBER_FEDERAL,'
      '                                      (select DH.CELL_PLAN_CODE'
      
        '                                         from DB_LOADER_ACCOUNT_' +
        'PHONE_HISTS dh                                   '
      
        '                                        where DH.PHONE_NUMBER = ' +
        'VOP.PHONE_NUMBER_FEDERAL '
      
        '                                          and DH.END_DATE >= ( N' +
        'VL(VOP.CONTRACT_CANCEL_DATE, sysdate) - 1)'
      
        '                                          and DH.BEGIN_DATE <= (' +
        ' NVL(VOP.CONTRACT_CANCEL_DATE, sysdate) - 1)'
      '                                          and rownum < 2'
      '                                      ),'
      
        '                                      (NVL(VOP.CONTRACT_CANCEL_D' +
        'ATE, sysdate) -1)'
      '                                   )'
      '               )  TARIFF_NAME              '
      '              ,('
      '                 SELECT sum( nvl(VAP.ABON_TP_NEW, 0)) '
      '                   FROM V_BILL_FINANCE_FOR_CLIENTS VAP'
      
        '                  WHERE VAP.PHONE_NUMBER = VOP.PHONE_NUMBER_FEDE' +
        'RAL'
      
        '                    AND VAP.YEAR_MONTH >= to_number( to_char( VO' +
        'P.CONTRACT_DATE, '#39'YYYYMM'#39') )  '
      
        '                    AND VAP.YEAR_MONTH <= to_number( to_char( NV' +
        'L(VOP.CONTRACT_CANCEL_DATE, sysdate), '#39'YYYYMM'#39') )'
      '                    AND VAP.ACCOUNT_ID = VOP.ACCOUNT_ID '
      '               ) ABON_TP_NEW'
      '              ,('
      
        '                 SELECT sum( nvl(VAP.BILL_SUM_NEW, 0)) --VOP.BIL' +
        'L_SUM_NEW - VOP.ROUMING_INTERNATIONAL - VOP.ROUMING_NATIONAL'
      '                   FROM V_BILL_FINANCE_FOR_CLIENTS VAP'
      
        '                  WHERE VAP.PHONE_NUMBER = VOP.PHONE_NUMBER_FEDE' +
        'RAL'
      
        '                    AND VAP.YEAR_MONTH >= to_number( to_char( VO' +
        'P.CONTRACT_DATE, '#39'YYYYMM'#39') )  '
      
        '                    AND VAP.YEAR_MONTH <= to_number( to_char( NV' +
        'L(VOP.CONTRACT_CANCEL_DATE, sysdate), '#39'YYYYMM'#39') )'
      '                    AND VAP.ACCOUNT_ID = VOP.ACCOUNT_ID '
      '               ) BILL_SUM_NEW'
      '              ,('
      
        '                 SELECT sum( nvl(VAP.ROUMING_INTERNATIONAL, 0)) ' +
        '--VOP.BILL_SUM_NEW - VOP.ROUMING_INTERNATIONAL - VOP.ROUMING_NAT' +
        'IONAL'
      '                   FROM V_BILL_FINANCE_FOR_CLIENTS VAP'
      
        '                  WHERE VAP.PHONE_NUMBER = VOP.PHONE_NUMBER_FEDE' +
        'RAL'
      
        '                    AND VAP.YEAR_MONTH >= to_number( to_char( VO' +
        'P.CONTRACT_DATE, '#39'YYYYMM'#39') )  '
      
        '                    AND VAP.YEAR_MONTH <= to_number( to_char( NV' +
        'L(VOP.CONTRACT_CANCEL_DATE, sysdate), '#39'YYYYMM'#39') )'
      '                    AND VAP.ACCOUNT_ID = VOP.ACCOUNT_ID '
      '               ) ROUMING_INTERNATIONAL'
      '              ,('
      
        '                 SELECT sum( nvl(VAP.ROUMING_NATIONAL, 0) ) --VO' +
        'P.BILL_SUM_NEW - VOP.ROUMING_INTERNATIONAL - VOP.ROUMING_NATIONA' +
        'L'
      '                   FROM V_BILL_FINANCE_FOR_CLIENTS VAP'
      
        '                  WHERE VAP.PHONE_NUMBER = VOP.PHONE_NUMBER_FEDE' +
        'RAL'
      
        '                    AND VAP.YEAR_MONTH >= to_number( to_char( VO' +
        'P.CONTRACT_DATE, '#39'YYYYMM'#39') )  '
      
        '                    AND VAP.YEAR_MONTH <= to_number( to_char( NV' +
        'L(VOP.CONTRACT_CANCEL_DATE, sysdate), '#39'YYYYMM'#39') )'
      '                    AND VAP.ACCOUNT_ID = VOP.ACCOUNT_ID '
      '               ) ROUMING_NATIONAL       '
      '              ,('
      
        '                 SELECT sum( nvl(VAP.BILL_SUM_OLD, 0) ) --VOP.BI' +
        'LL_SUM_NEW - VOP.ROUMING_INTERNATIONAL - VOP.ROUMING_NATIONAL'
      '                   FROM V_BILL_FINANCE_FOR_CLIENTS VAP'
      
        '                  WHERE VAP.PHONE_NUMBER = VOP.PHONE_NUMBER_FEDE' +
        'RAL'
      
        '                    AND VAP.YEAR_MONTH >= to_number( to_char( VO' +
        'P.CONTRACT_DATE, '#39'YYYYMM'#39') )  '
      
        '                    AND VAP.YEAR_MONTH <= to_number( to_char( NV' +
        'L(VOP.CONTRACT_CANCEL_DATE, sysdate), '#39'YYYYMM'#39') )'
      '                    AND VAP.ACCOUNT_ID = VOP.ACCOUNT_ID '
      '               ) BILL_SUM_OLD'
      '        from V_OTTOK_PHONES_MONTH vop        '
      '            ,( select AC.ACCOUNT_ID '
      '                 from accounts ac'
      
        '                where ('#39','#39'||:pACCOUNT_LIST||'#39','#39' like '#39'%,'#39'||ac.AC' +
        'COUNT_ID||'#39',%'#39' or '#39','#39'||:pACCOUNT_LIST||'#39','#39' = '#39',-1,'#39')'
      '             ) AC '
      '        where VOP.YEAR_MONTH >= :pYEAR_MONTH_BEGIN'
      '          and VOP.YEAR_MONTH <= :pYEAR_MONTH_END'
      '          and VOP.ACCOUNT_ID = AC.ACCOUNT_ID'
      ') q'
      'order by q.YEAR_MONTH')
    FetchAll = True
    Left = 72
    Top = 432
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'pACCOUNT_LIST'
      end
      item
        DataType = ftUnknown
        Name = 'pYEAR_MONTH_BEGIN'
      end
      item
        DataType = ftUnknown
        Name = 'pYEAR_MONTH_END'
      end>
  end
