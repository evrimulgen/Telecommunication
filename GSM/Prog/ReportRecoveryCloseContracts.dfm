  object qReportRecoveryCloseContracts: TOraQuery
    Session = MainForm.OraSession
    SQL.Strings = (
      'SELECT Q.YEAR_MONTH'
      '      ,('
      '        SELECT COUNT(DISTINCT VC.PHONE_NUMBER_FEDERAL)'
      '          FROM V_CONTRACTS VC'
      '              ,DB_LOADER_ACCOUNT_PHONES dla'
      
        '         WHERE TO_NUMBER(TO_CHAR(VC.CONTRACT_DATE, '#39'YYYYMM'#39')) <=' +
        ' Q.YEAR_MONTH'
      
        '           AND (TO_NUMBER(TO_CHAR(VC.CONTRACT_CANCEL_DATE, '#39'YYYY' +
        'MM'#39')) >= Q.YEAR_MONTH'
      '               OR '
      '                VC.CONTRACT_CANCEL_DATE IS NULL'
      '               ) '
      '           and dla.PHONE_NUMBER = VC.PHONE_NUMBER_FEDERAL'
      
        '           and ('#39','#39'||:pACCOUNT_LIST||'#39','#39' like '#39'%,'#39'||dla.ACCOUNT_' +
        'ID||'#39',%'#39' or '#39','#39'||:pACCOUNT_LIST||'#39','#39' = '#39',-1,'#39')'
      '           and DLA.YEAR_MONTH = Q.YEAR_MONTH '
      '       ) ABONENT_COUNT'
      '      ,('
      '        SELECT COUNT(DISTINCT VC.PHONE_NUMBER_FEDERAL)'
      '          FROM V_CONTRACTS VC'
      '              ,DB_LOADER_ACCOUNT_PHONES dla'
      
        '         WHERE TO_NUMBER(TO_CHAR( VC.CONTRACT_DATE , '#39'YYYYMM'#39')) ' +
        '<= TO_NUMBER(TO_CHAR(ADD_MONTHS(TO_DATE(TO_CHAR(Q.YEAR_MONTH), '#39 +
        'yyyymm'#39'),-1), '#39'yyyymm'#39'))'
      
        '           AND (TO_NUMBER(TO_CHAR(VC.CONTRACT_CANCEL_DATE, '#39'YYYY' +
        'MM'#39')) >= TO_NUMBER(TO_CHAR(ADD_MONTHS(TO_DATE(TO_CHAR(Q.YEAR_MON' +
        'TH), '#39'yyyymm'#39'),-1), '#39'yyyymm'#39'))'
      '               OR '
      '                VC.CONTRACT_CANCEL_DATE IS NULL'
      '               ) '
      '           and dla.PHONE_NUMBER = VC.PHONE_NUMBER_FEDERAL'
      
        '           and ('#39','#39'||:pACCOUNT_LIST||'#39','#39' like '#39'%,'#39'||dla.ACCOUNT_' +
        'ID||'#39',%'#39' or '#39','#39'||:pACCOUNT_LIST||'#39','#39' = '#39',-1,'#39')'
      
        '           and DLA.YEAR_MONTH = TO_NUMBER(TO_CHAR(ADD_MONTHS(TO_' +
        'DATE(TO_CHAR(Q.YEAR_MONTH), '#39'yyyymm'#39'),-1), '#39'yyyymm'#39')) '
      '       ) ABONENT_COUNT_PREV_MONTH'
      '      ,('
      '        SELECT COUNT(DISTINCT VC.PHONE_NUMBER_FEDERAL)'
      '          FROM V_CONTRACTS VC'
      '              ,DB_LOADER_ACCOUNT_PHONES dla'
      
        '         WHERE TO_NUMBER(TO_CHAR(VC.CONTRACT_DATE, '#39'YYYYMM'#39')) = ' +
        'Q.YEAR_MONTH'
      '           and dla.PHONE_NUMBER = VC.PHONE_NUMBER_FEDERAL'
      
        '           and ('#39','#39'||:pACCOUNT_LIST||'#39','#39' like '#39'%,'#39'||dla.ACCOUNT_' +
        'ID||'#39',%'#39' or '#39','#39'||:pACCOUNT_LIST||'#39','#39' = '#39',-1,'#39')'
      
        '           and DLA.YEAR_MONTH = Q.YEAR_MONTH                    ' +
        '   '
      '       ) CONNECTION_COUNT'
      '      ,('
      '        SELECT COUNT(DISTINCT DLA.PHONE_NUMBER)'
      '          FROM DB_LOADER_ACCOUNT_PHONES DLA'
      '         WHERE DLA.PHONE_IS_ACTIVE = 1'
      '           and dla.YEAR_MONTH = Q.YEAR_MONTH'
      
        '           and ('#39','#39'||:pACCOUNT_LIST||'#39','#39' like '#39'%,'#39'||dla.ACCOUNT_' +
        'ID||'#39',%'#39' or '#39','#39'||:pACCOUNT_LIST||'#39','#39' = '#39',-1,'#39')'
      '       ) ACTIVE_ABONENT_COUNT'
      '      ,(select COUNT(DISTINCT VC.PHONE_NUMBER_FEDERAL)'
      '        from v_contracts vc'
      '            ,DB_LOADER_ACCOUNT_PHONES DLA'
      '        where '
      
        '            TO_NUMBER(TO_CHAR(VC.CONTRACT_DATE, '#39'YYYYMM'#39')) <= Q.' +
        'YEAR_MONTH'
      
        '          and TO_NUMBER(TO_CHAR(nvl(VC.CONTRACT_CANCEL_DATE, sys' +
        'date), '#39'YYYYMM'#39')) >= Q.YEAR_MONTH'
      '          and VC.PHONE_NUMBER_FEDERAL = dla.PHONE_NUMBER'
      '          and DLA.YEAR_MONTH = Q.YEAR_MONTH'
      
        '          and ('#39','#39'||:pACCOUNT_LIST||'#39','#39' like '#39'%,'#39'||dla.ACCOUNT_I' +
        'D||'#39',%'#39' or '#39','#39'||:pACCOUNT_LIST||'#39','#39' = '#39',-1,'#39')'
      '          and ('
      
        '                -- '#1085#1080#1078#1077' '#1073#1083#1086#1082', '#1074' '#1082#1086#1090#1086#1088#1086#1084' '#1074#1099#1095#1080#1089#1083#1103#1102#1090#1089#1103' '#1090#1077' '#1085#1086#1084#1077#1088', '#1082#1086 +
        #1090#1086#1088#1099#1077' '#1087#1086#1087#1072#1083#1080' '#1074' '#1086#1090#1090#1086#1082' '#1085#1072#1093#1086#1076#1103#1089#1100' 2 '#1084#1077#1089#1103#1094#1077' '#1074' '#1084#1080#1085#1091#1089#1077' '#1089' '#1080' '#1079#1072#1073#1083#1086#1082#1080#1088#1086#1074#1072#1085 +
        #1099' '
      '                     (      '
      '                         not exists'
      '                                  ('
      '                                   select 1'
      
        '                                     from DB_LOADER_ACCOUNT_PHON' +
        'E_HISTS dla'
      
        '                                    where DLA.PHONE_NUMBER = VC.' +
        'PHONE_NUMBER_FEDERAL'
      
        '                                      and DLA.PHONE_IS_ACTIVE = ' +
        '1'
      
        '                                      and DLA.BEGIN_DATE < ADD_M' +
        'ONTHS (TO_DATE(Q.YEAR_MONTH,'#39'YYYYmm'#39'), 1) '
      
        '                                      and DLA.END_DATE >= ADD_MO' +
        'NTHS (TO_DATE(Q.YEAR_MONTH,'#39'YYYYmm'#39'), -2) --Q.YEAR_MONTH'
      '                                  )'
      '                      and not exists'
      '                                  ('
      '                                   select 1'
      '                                     FROM IOT_BALANCE_HISTORY bh'
      
        '                                    WHERE BH.PHONE_NUMBER = VC.P' +
        'HONE_NUMBER_FEDERAL                '
      '                                      AND BH.BALANCE >0'
      
        '                                      AND BH.LAST_UPDATE >= ADD_' +
        'MONTHS (TO_DATE(Q.YEAR_MONTH,'#39'YYYYmm'#39'), -2)'
      
        '                                      AND BH.LAST_UPDATE <= TO_D' +
        'ATE(Q.YEAR_MONTH,'#39'YYYYmm'#39') '
      '                                  )'
      
        '                      and exists  -- '#1076#1083#1103' '#1090#1086#1075#1086', '#1095#1090#1086#1073#1099' '#1090#1077' '#1085#1086#1084#1077#1088#1072', ' +
        #1082#1086#1090#1086#1088#1099#1077' '#1091#1078#1077' '#1087#1086#1087#1072#1083#1080' '#1074' '#1086#1090#1090#1086#1082' '#1074' '#1087#1088#1077#1076#1099#1076#1091#1097#1077#1084' '#1084#1077#1089#1103#1094#1077', '#1085#1077' '#1087#1086#1087#1072#1076#1072#1083#1080' '#1074' '#1086#1090 +
        #1090#1086#1082' '#1074' '#1089#1083#1077#1076#1091#1102#1097#1077#1084' '#1084#1077#1089#1103#1094#1077
      '                                  ('
      '                                   select 1'
      
        '                                     from DB_LOADER_ACCOUNT_PHON' +
        'E_HISTS dla'
      
        '                                    where DLA.PHONE_NUMBER = VC.' +
        'PHONE_NUMBER_FEDERAL'
      
        '                                      and DLA.PHONE_IS_ACTIVE = ' +
        '1'
      
        '                                      and DLA.BEGIN_DATE < ADD_M' +
        'ONTHS (TO_DATE(Q.YEAR_MONTH,'#39'YYYYmm'#39'), 1) '
      
        '                                      and DLA.END_DATE >= ADD_MO' +
        'NTHS (TO_DATE(Q.YEAR_MONTH,'#39'YYYYmm'#39'), -3) --Q.YEAR_MONTH'
      '                                  )'
      
        '                      and exists -- '#1076#1083#1103' '#1090#1086#1075#1086', '#1095#1090#1086#1073#1099' '#1090#1077' '#1085#1086#1084#1077#1088#1072', '#1082 +
        #1086#1090#1086#1088#1099#1077' '#1091#1078#1077' '#1087#1086#1087#1072#1083#1080' '#1074' '#1086#1090#1090#1086#1082' '#1074' '#1087#1088#1077#1076#1099#1076#1091#1097#1077#1084' '#1084#1077#1089#1103#1094#1077', '#1085#1077' '#1087#1086#1087#1072#1076#1072#1083#1080' '#1074' '#1086#1090#1090 +
        #1086#1082' '#1074' '#1089#1083#1077#1076#1091#1102#1097#1077#1084' '#1084#1077#1089#1103#1094#1077
      '                                  ('
      '                                   select 1'
      '                                     FROM IOT_BALANCE_HISTORY bh'
      
        '                                    WHERE BH.PHONE_NUMBER = VC.P' +
        'HONE_NUMBER_FEDERAL                '
      '                                      AND BH.BALANCE >0'
      
        '                                      AND BH.LAST_UPDATE >= ADD_' +
        'MONTHS (TO_DATE(Q.YEAR_MONTH,'#39'YYYYmm'#39'), -3)'
      
        '                                      AND BH.LAST_UPDATE <= TO_D' +
        'ATE(Q.YEAR_MONTH,'#39'YYYYmm'#39') '
      '                                  )'
      '                     )    '
      '                   or'
      
        '               -- '#1053#1080#1078#1077' '#1089#1095#1080#1090#1072#1102#1090#1089#1103' '#1090#1077' '#1085#1086#1084#1077#1088#1072', '#1082#1086#1090#1086#1088#1099#1077' '#1073#1099#1083#1080' '#1085#1077#1072#1082#1090#1080#1074 +
        #1085#1099' '#1074' '#1090#1077#1095#1077#1085#1080#1080' 3-'#1093' '#1084#1077#1089#1103#1094#1077#1074' '#1087#1086#1076#1088#1103#1076
      '                     (not exists '
      '                                ('
      '                                 select 1 '
      
        '                                   from CALL_SUMMARY cs -- '#1087#1091#1089#1090#1072 +
        #1103' '#1076#1077#1090#1072#1083#1082#1072
      
        '                                  where CS.YEAR_MONTH <= Q.YEAR_' +
