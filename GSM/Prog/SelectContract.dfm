  object qRightAccountAllow: TOraQuery
    DataTypeMap = <>
    Session = MainForm.OraSession
    SQL.Strings = (
      
        'SELECT count(*) cnt FROM rights_type_account_allow r, accounts a' +
        ', user_names u'
      
        'WHERE r.rights_type = u.rights_type AND r.account_id = a.account' +
        '_id AND a.account_id = GET_ACCOUNT_ID_BY_PHONE(:phone_number) AN' +
