  object qMain: TOraQuery
    KeyFields = 'CONTRACT_ID'
    DataTypeMap = <>
    SQL.Strings = (
      'SELECT C.CONTRACT_NUM,'
      '       C.CONTRACT_DATE,'
      '       O.OPERATOR_NAME,'
      '       AP.PHONE_NUMBER_FEDERAL,'
      '       CC.CONTRACT_CANCEL_DATE,'
      '       A.SURNAME ||'#39' '#39'|| A.NAME || '#39' '#39' || A.PATRONYMIC AS FIO,'
      '       C.CONTRACT_ID,'
      '       C.HAND_BLOCK,'
      '       A.SURNAME,'
      '       A.NAME,'
      '       A.PATRONYMIC'
      '     '
      '  FROM CONTRACTS C,'
      '       CONTRACT_CANCELS CC,'
      '       ABONENTS A,'
      '       OPERATORS O,'
      '       (SELECT'
      '          PHONE_NUMBER PHONE_NUMBER_FEDERAL'
      '          FROM DB_LOADER_ACCOUNT_PHONES'
      '          where'
      '          DB_LOADER_ACCOUNT_PHONES.Phone_Number is not null'
      '         GROUP BY PHONE_NUMBER) AP'
      '  WHERE AP.PHONE_NUMBER_FEDERAL=C.PHONE_NUMBER_FEDERAL(+)'
      '    AND C.CONTRACT_ID=CC.CONTRACT_ID(+)  '
      '    AND C.ABONENT_ID=A.ABONENT_ID(+)'
      '    AND C.OPERATOR_ID=O.OPERATOR_ID(+)'
      '  ORDER BY ap.PHONE_NUMBER_FEDERAL ASC, C.CONTRACT_DATE DESC')
    FetchRows = 1000
    ReadOnly = True
    RefreshOptions = [roAfterInsert, roAfterUpdate]
    AfterOpen = qMainAfterOpen
    Left = 264
    Top = 73
  end
