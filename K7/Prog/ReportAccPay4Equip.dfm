  inherited qReport: TOraQuery
    SQL.Strings = (
      'SELECT '
      '   eq.ID_ACC_PAYMENT_EQUIPMENT, eq.ACC_PAYMENT_NUMBER, '
      '   eq.ACCOUNT_ID, AC.ACCOUNT_NUMBER, AC.COMPANY_NAME,'
      '   eq.DATE_ISSUING, eq.ACC_PAYMENT_SUM, eq.ACC_PAYMENT_CHECK, '
      '   eq.USER_CREATED, eq.DATE_CREATED, eq.USER_LAST_UPDATED, '
      '   eq.DATE_LAST_UPDATED'
      'FROM ACC_PAYMENTS_FOR_EQUIPMENT eq'
      'LEFT JOIN ACCOUNTS ac ON eq.ACCOUNT_ID = ac.ACCOUNT_ID'
      'WHERE ((:ACCOUNT_ID IS NULL) OR (eq.ACCOUNT_ID=:ACCOUNT_ID))'
      'ORDER BY eq.DATE_ISSUING DESC')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ACCOUNT_ID'
      end>
