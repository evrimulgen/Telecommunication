  object qPeriods: TOraQuery
    SQL.Strings = (
      'SELECT '
      
        ' substr(TO_CHAR(YEAR_MONTH),1,4)||'#39'_'#39'|| substr(TO_CHAR(YEAR_MONT' +
        'H),-2) as SMONTH'
      'FROM (SELECT YEAR_MONTH '
      '        FROM DB_LOADER_BILLS'
      '      UNION ALL  '
      '      SELECT YEAR_MONTH '
      '        FROM DB_LOADER_FULL_FINANCE_BILL)'
      'GROUP BY YEAR_MONTH'
      'ORDER BY YEAR_MONTH DESC')
    Left = 280
    Top = 152
  end
