  inherited qMain: TOraQuery
    UpdatingTable = 'CONTRACT_GROUPS'
    KeyFields = 'GROUP_ID'
    KeySequence = 'S_NEW_CONTRACT_GROUP_ID'
    SQLInsert.Strings = (
      'INSERT INTO CONTRACT_GROUPS'
      '  (GROUP_ID, GROUP_NAME)'
      'VALUES'
      '  (:GROUP_ID, :GROUP_NAME)'
      'RETURNING'
      '  GROUP_ID, GROUP_NAME'
      'INTO'
      '  :GROUP_ID, :GROUP_NAME')
    SQLDelete.Strings = (
      'DELETE FROM CONTRACT_GROUPS'
      'WHERE'
      '  GROUP_ID = :Old_GROUP_ID')
    SQLUpdate.Strings = (
      'UPDATE CONTRACT_GROUPS'
      'SET'
      '  GROUP_ID = :GROUP_ID, GROUP_NAME = :GROUP_NAME'
      'WHERE'
      '  GROUP_ID = :Old_GROUP_ID'
      'RETURNING'
      '  GROUP_ID, GROUP_NAME'
      'INTO'
      '  :GROUP_ID, :GROUP_NAME')
    SQLLock.Strings = (
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      'SELECT G.*,'
      
        '  (SELECT NVL(SUM(get_abonent_balance(phone_number_federal, cont' +
        'ract_cancel_date)),0) group_balance '
      '   FROM v_contracts c'
      '   WHERE group_id = g.group_id) balance,'
      
        '  (SELECT count(*) FROM contracts c WHERE c.group_id = g.group_i' +
        'd) cnt'
      'FROM CONTRACT_GROUPS G'
      'WHERE'
      '  GROUP_ID = :GROUP_ID')
    SQL.Strings = (
      'SELECT g.group_id, g.group_name, '
      
        '  (SELECT NVL(SUM(get_abonent_balance(phone_number_federal, cont' +
        'ract_cancel_date)),0) group_balance '
      '   FROM v_contracts c'
      '   WHERE group_id = g.group_id) balance,'
      
        '  (SELECT count(*) FROM contracts c WHERE c.group_id = g.group_i' +
        'd) cnt'
      'FROM CONTRACT_GROUPS g'
      '--ORDER BY 1')
