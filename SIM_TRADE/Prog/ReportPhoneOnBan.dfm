  object qBeeline_Loader_Inf: TOraQuery
    SQL.Strings = (
      'SELECT a.ACCOUNT_NUMBER,'
      '       bee.PHONE_NUMBER, '
      '       bee.OBJ_ID, '
      '       bee.BAN'
      ''
      ' FROM BEELINE_LOADER_INF  bee, ACCOUNTS a '
      ' where'
      '    bee.ACCOUNT_ID= a.ACCOUNT_ID and'
      '    ('
      '        (:IS_COLLECTOR IS NULL) OR --'#1074#1099#1073#1080#1088#1072#1077#1084' '#1074#1089#1077' '#1089#1095#1077#1090#1072
      
        '        (a.IS_COLLECTOR = :IS_COLLECTOR) OR --'#1074#1099#1073#1080#1088#1072#1077#1084' '#1090#1086#1083#1100#1082#1086' '#1082#1086 +
        #1083#1083#1077#1082#1090#1086#1088
      
        '        (:IS_COLLECTOR IS NOT NULL AND :IS_COLLECTOR <> 1) --'#1074#1099#1073 +
        #1080#1088#1072#1077#1084' '#1074#1089#1077' '#1073#1077#1079' '#1082#1086#1083#1083#1077#1090#1082#1086#1088#1072
      '    )'
      ''
      'and rownum <= 1')
    Left = 168
    Top = 104
    ParamData = <
      item
        DataType = ftInteger
        Name = 'IS_COLLECTOR'
        ParamType = ptInput
      end>
